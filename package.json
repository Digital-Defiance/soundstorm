{
  "name": "@soundstorm/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx nx build soundstorm-lib && npx nx build soundstorm-react && npx nx build soundstorm",
    "build:dev": "npx nx build soundstorm-lib --configuration=development && npx nx build soundstorm-react --configuration=development && npx nx build soundstorm --configuration=development",
    "clean": "rm -rf dist",
    "serve": "npx nx serve soundstorm",
    "serve:dev": "npx nx serve soundstorm --configuration=development",
    "build-serve:dev": "npx nx build soundstorm-lib --configuration=development && npx nx build soundstorm-react --configuration=development && npx nx serve soundstorm --configuration=development",
    "lint:fix": "npx nx run-many --all --target=lint --fix",
    "new:dev-https-cert": "openssl req -x509 -nodes -newkey rsa:4096 -keyout apps/soundstorm/localdev/cert.key -out apps/soundstorm/localdev/cert.pem -days 365",
    "new:secret": "node -e \"console.log (require('crypto').randomBytes(32).toString ('hex'));\""
  },
  "private": true,
